<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ar.com.imaginatic.nocv.domain">

	

	<class name="Skill" table="SKILL">
		<id name="oid" type="string" column="ID_SKILL" >
			<generator class="assigned"></generator>
		</id>

		<property name="descripcion" column="descripcion" length="100" />	
		
	</class>



    <query name="findAllSkills">
        <![CDATA[from Skill u]]>
    </query>

</hibernate-mapping>